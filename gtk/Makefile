basic:basic.vala
	valac --pkg gtk+-3.0 basic.vala
sync:sync.vala
	valac --pkg gtk+-3.0 sync.vala
textViewer:textViewer.vala
	valac --pkg gtk+-3.0 textViewer.vala
dialog:dialog.vala
	valac --pkg gtk+-3.0 dialog.vala
